    "@stripe/stripe-js": "^7.3.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/nodemailer": "^6.4.15",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
