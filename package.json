{
  "name": "ada-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "process-images": "node src/scripts/process-images.js",
    "convert-images": "node src/scripts/convert-to-webp.js",
    "sync-images": "powershell -Command \"$changes = aws s3 sync ./public s3://americandefensealliance --delete --cache-control 'public, max-age=0, must-revalidate' --exclude '*.pdf' --output text --profile ada; if ($changes) { $paths = ($changes | Select-String 'upload:' | ForEach-Object { if($_ -match 'public\\\\(.+?) to') { '/' + $matches[1].Replace('\\\\', '/') } }); if ($paths) { Write-Host 'Creating CloudFront invalidation for:'; Write-Host $paths; aws cloudfront create-invalidation --distribution-id E15R7SXII9T1VC --paths $paths --profile ada } }\"",
    "create-event": "node scripts/createEvent.js",
    "import-speakers": "node scripts/speaker-update.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.835.0",
