{
  "name": "ada-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "process-images": "node src/scripts/process-images.js",
    "convert-images": "node src/scripts/convert-to-webp.js",
    "sync-images": "powershell -Command \"$changes = aws s3 sync ./public s3://americandefensealliance --delete --cache-control 'public, max-age=0, must-revalidate' --output text; if ($changes) { $paths = ($changes | Select-String 'upload:' | ForEach-Object { if($_ -match 'public\\\\(.+?) to') { '/' + $matches[1].Replace('\\', '/') } }); if ($paths) { Write-Host 'Creating CloudFront invalidation for:'; Write-Host $paths; aws cloudfront create-invalidation --distribution-id E15R7SXII9T1VC --paths $paths } }\"",
    "create-event": "node scripts/createEvent.js",
    "import-speakers": "node scripts/speaker-update.js" 
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.697.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@icons-pack/react-simple-icons": "^10.2.0",
    "@next/third-parties": "^14.2.11",
    "@react-google-maps/api": "^2.19.3",
    "@tailwindcss/typography": "^0.5.14",
    "@types/nodemailer": "^6.4.15",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^1.7.9",
    "dompurify": "^3.1.6",
    "embla-carousel": "^8.4.0",
    "embla-carousel-auto-height": "^8.4.0",
    "embla-carousel-autoplay": "^8.4.0",
    "embla-carousel-react": "^8.4.0",
    "lucide-react": "^0.435.0",
    "mime-types": "^2.1.35",
    "next": "14.2.6",
    "nodemailer": "^6.9.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-masonry-css": "^1.0.16",
    "react-modal-video": "^2.0.2",
    "react-slideshow-image": "^4.3.2",
    "sharp": "^0.33.5",
    "tailwind": "^4.0.0",
    "yet-another-react-lightbox": "^3.21.7"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
